 <h1>Add a sheep to rent</h1>
      <%= simple_form_for [@sheep] do |f| %>
        <%= f.association :user %>
        <%= f.input :name %>
        <%= f.input :image_url %>
        <%= f.input :location %>
        <%= f.input :description %>
        <%= f.input :weight %>
        <%= f.input :breed %>
        <%= f.input :day_rate %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
